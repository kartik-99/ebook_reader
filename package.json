{
  "name": "ebook_reader",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "clean:android": "echo 'require(\"child_process\").execSync(\"cd android; ./gradlew clean\", {stdio: \"inherit\"})' | node ",
    "clean:ios": "echo 'require(\"child_process\").execSync(\"cd ios; xcodebuild clean\", {stdio: \"inherit\"})' | node ",
    "clean": "npm run clean:android && npm run clean:ios",
    "clean:extreme": "npm run clean && rm -rf node_modules && rm -rf ios/Pods",
    "ia": "yarn install && cd ios && pod install && cd .. ",
    "resetHard": "yarn clean:extreme && yarn ia",
    "s": "yarn start --reset-cache"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.12.1",
    "@react-native-community/masked-view": "^0.1.11",
    "@react-native-community/slider": "4.0.0-rc.3",
    "@react-native-picker/picker": "^2.2.1",
    "@react-navigation/native": "^5.9.8",
    "@react-navigation/stack": "^5.14.9",
    "fast-xml-parser": "^3.21.1",
    "react": "17.0.2",
    "react-native": "0.66.4",
    "react-native-code-push": "^7.0.4",
    "react-native-device-info": "^8.4.8",
    "react-native-document-picker": "^5.2.0",
    "react-native-drawer": "^2.5.1",
    "react-native-elements": "^3.4.2",
    "react-native-fs": "^2.18.0",
    "react-native-gesture-handler": "^1.10.3",
    "react-native-gifted-charts": "^0.1.15",
    "react-native-pager-view": "^5.4.9",
    "react-native-reanimated": "^2.2.4",
    "react-native-safe-area-context": "^3.3.2",
    "react-native-screens": "^3.10.2",
    "react-native-static-server": "^0.5.0",
    "react-native-super-grid": "^4.2.0",
    "react-native-tab-view": "^3.1.1",
    "react-native-vector-icons": "^8.1.0",
    "react-native-webview": "^11.6.4",
    "react-native-zip-archive": "^6.0.6",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-persist": "^6.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.64.0",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
